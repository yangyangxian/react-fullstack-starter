{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm --prefix ../common run watch\" \"npm run dev:alone\"",
    "dev:alone": "vite",
    "build": "npm --prefix ../common run build && npm run build:alone",
    "build:alone": "cross-env NODE_ENV=production vite build",
    "lint": "eslint .",
    "preview": "npm --prefix ../common run build && vite preview",
    "shutdown": "echo 'Gracefully shutting down...'"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  }
}
